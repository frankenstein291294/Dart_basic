void main() {
  
  final wolverine = new Heroe('Logan', 'Regeneracion');
  print(wolverine);
  
}

class Heroe {
  String nombre;
  String poder;
  
  Heroe(String nombre, String poder) {
    this.nombre = nombre;
    this.poder = poder;
  }
  
  String toString() {
    return '${this.nombre} - ${this.poder}';
  }
}
